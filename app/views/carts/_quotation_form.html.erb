<div class="row">
<div class="span6 offset3">
<%= form_for @order, :url => { :action => "create_quotation" } do |f| %>
<%= f.label :code, "Código" %>
<%= f.text_field :code %>
<%= f.label :name, "Nombre Cliente" %>
<%= f.text_field :name %>
<%= f.label :rut, "RUT" %>
<%= f.text_field :rut %>
<%= f.label :address, "Dirección" %>
<%= f.text_field :address %>
<%= f.label :email, "E-mail" %>
<%= f.text_field :email %>
<%= f.label :telephone, "Teléfono" %>
<%= f.text_field :telephone %>
<%= f.label :sell_condition, "Condición de venta" %>
<%= f.text_field :sell_condition %>
<%= f.submit "Crear cotización", class: "btn btn-large btn-primary" %>
<% end %>
</div>
</div>
